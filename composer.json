{
    "name": "blackfireio/blackfire-workshop",
    "description": "Sasquatch Blackfire Workshop",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "blackfireio/blackfire-symfony-meta": "^1.0",
        "doctrine/annotations": "^2.0",
        "doctrine/doctrine-bundle": "^2.12",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^3.2",
        "knplabs/knp-time-bundle": "^2.2",
        "nelmio/cors-bundle": "^2.4",
        "phpdocumentor/reflection-docblock": "^5.4",
        "phpstan/phpdoc-parser": "^1.29",
        "symfony/asset": "7.0.*",
        "symfony/console": "7.0.*",
        "symfony/doctrine-bridge": "7.0.*",
        "symfony/dotenv": "7.0.*",
        "symfony/flex": "^2",
        "symfony/form": "7.0.*",
        "symfony/framework-bundle": "7.0.*",
        "symfony/html-sanitizer": "7.0.*",
        "symfony/http-client": "7.0.*",
        "symfony/intl": "7.0.*",
        "symfony/monolog-bundle": "^3.10",
        "symfony/polyfill-intl-messageformatter": "^1.29",
        "symfony/property-access": "7.0.*",
        "symfony/runtime": "7.0.*",
        "symfony/security-bundle": "7.0.*",
        "symfony/serializer": "7.0.*",
        "symfony/stimulus-bundle": "*",
        "symfony/ux-live-component": "*",
        "symfony/ux-twig-component": "*",
        "symfony/webpack-encore-bundle": "*",
        "symfony/yaml": "7.0.*",
        "symfonycasts/sass-bundle": "^0.7.0",
        "symfonycorp/platformsh-meta": "^1.0",
        "twig/extra-bundle": "^3.10",
        "twig/intl-extra": "^3.10",
        "twig/markdown-extra": "^3.10",
        "webonyx/graphql-php": "^15.11"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true,
        "platform": {
            "php": "8.3"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.0.*"
        }
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "*",
        "fakerphp/faker": "^1.23",
        "symfony/debug-bundle": "7.0.*",
        "symfony/maker-bundle": "^1.59",
        "symfony/phpunit-bridge": "^7.0",
        "symfony/web-profiler-bundle": "7.0.*"
    }
}
